# Server Configuration
PORT=3001
NODE_ENV=development

# API Configuration
API_PREFIX=api
API_VERSION=1

# CORS Configuration
CORS_ORIGIN=http://localhost:3000
CORS_METHODS=GET,HEAD,PUT,PATCH,POST,DELETE

# Rate Limiting
THROTTLE_TTL=60
THROTTLE_LIMIT=100

# Logging
LOG_LEVEL=debug
LOG_PRETTY_PRINT=true

# Health Check
HEALTH_CHECK_URL=/health

# Security
HELMET_ENABLED=true
RATE_LIMIT_ENABLED=true
